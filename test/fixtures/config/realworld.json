{
   "apis":{
      "createArticle":{
         "method":"post",
         "uri":"/articles"
      },
      "createComment":{
         "method":"post",
         "uri":"/articles/{slug}/comments"
      },
      "deleteArticle":{
         "method":"delete",
         "uri":"/articles/{slug}"
      },
      "deleteComment":{
         "method":"delete",
         "uri":"/articles/{slug}/comments/{id}"
      },
      "favoriteArticle":{
         "method":"post",
         "uri":"/articles/{slug}/favorite"
      },
      "followProfile":{
         "method":"post",
         "uri":"/profiles/{id}/follow"
      },
      "getArticle":"/articles/{slug}",
      "getFeed":"/articles/feed",
      "getProfile":"/profiles/{id}",
      "getUser":"/user",
      "listArticles":"/articles",
      "listCommentsForActicle":"/articles/{slug}/comments",
      "listTags":"/tags",
      "login":{
         "method":"post",
         "uri":"/users/login"
      },
      "register":{
         "method":"post",
         "uri":"/users"
      },
      "unfavoriteArticle":{
         "method":"delete",
         "uri":"/articles/{slug}/favorite"
      },
      "unfollowProfile":{
         "method":"delete",
         "uri":"/profiles/{id}/follow"
      },
      "updateArticle":{
         "method":"put",
         "uri":"/articles/{slug}"
      },
      "updateUser":{
         "method":"put",
         "uri":"/user"
      }
   },
   "plugins":[
      "hest-plugin-builtin"
   ],
   "rootDir":".",
   "serializer":"json",
   "sessionFile":"/tmp/realworld.session",
   "url":"http://localhost:3000/api",
   "variables":{

   }
}
